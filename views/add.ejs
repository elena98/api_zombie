<%include _header%>
<h1>Registro de armas</h1>
<h3>Completar los campos con la informacion correspondiente.<h3>

<form action="/add" method="post">
<div class="form-group">
    <input name= "descripcion" type= "text" class= "form-control" placeholder= "DESCRIPCION" required autofocus>
</div>
<div class="form-group">
    <input name= "categoria" type= "text" class= "form-control" placeholder= "CATEGORIA" required autofocus>
</div>
<div class="form-group"> 
    <input name= "fuerza" type= "number" class= "form-control" placeholder= "NUMERO DE FUERZA" required autofocus>
</div>
<div class="form-group">
    <input name= "municiones" type= "boolean" class= "form-control" placeholder= "TRUE" required autofocus>
</div>

<br>
<br>
<br>
<input type= "submit" value= "Registrar nueva arma" class= "btn btn-primary btn-block">
</form>

<%include _footer%>